{% block sw_condition_base_fields %}
    {% block sw_condition_shipping_street_field_operator %}
        <sw-condition-operator-select operatorSetName="string"
                                      v-model="condition.value.operator">
        </sw-condition-operator-select>
    {% endblock %}
    {% block sw_condition_shipping_street_field_street_name %}
        <sw-field class="field--main"
                  name="streetName"
                  size="medium"
                  v-model="condition.value.streetName">
        </sw-field>
    {% endblock %}
{% endblock %}
