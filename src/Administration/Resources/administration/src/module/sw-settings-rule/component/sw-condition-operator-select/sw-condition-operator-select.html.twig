{% block sw_condition_operator_select %}
    <sw-select :id="createId()"
               itemValueKey="identifier"
               displayName="label"
               :store="operators"
               required
               v-model="operatorValue"
               class="sw-condition-operator-select has--arrow"
               @input="$emit('input', $event)">
    </sw-select>
{% endblock %}