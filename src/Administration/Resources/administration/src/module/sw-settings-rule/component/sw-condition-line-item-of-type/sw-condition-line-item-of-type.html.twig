{% block sw_condition_base_fields %}
    {% block sw_condition_line_item_of_type_field_operator %}
        <sw-condition-operator-select operatorSetName="string"
                                      v-model="condition.value.operator">
        </sw-condition-operator-select>
    {% endblock %}
    {% block sw_condition_line_item_of_type_field_line_item_type %}
        <sw-select :id="createId()"
                   name="lineItemType"
                   itemValueKey="value"
                   displayName="label"
                   required
                   :store="lineItemTypeStore"
                   v-model="condition.value.lineItemType"
                   class="field--main">
        </sw-select>
    {% endblock %}
{% endblock %}
