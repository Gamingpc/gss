{% block sw_condition_base_fields %}
    {% block sw_condition_not_found_field_error %}
        <sw-field type="text"
                  class="field--main field--error"
                  :placeholder="$tc('global.sw-condition.condition.not-found.label')"
                  :disabled="true">
            <template #suffix>
                <sw-help-text :text="errorMessage" :width="200" tooltipPosition="top">
                </sw-help-text>
            </template>
        </sw-field>
    {% endblock %}
{% endblock %}